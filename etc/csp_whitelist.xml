<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="scriptsrc" type="host">*.payu.in</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="payu" type="host">*.payu.in</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="cntsrc" type="host">*.payu.in</value>
            </values>
        </policy>
         <policy id="img-src">
            <values>
                <value id="payu" type="host">*.payu.in</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="payu" type="host">*.payu.in</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="dta" type="host">'self'</value>
		<value id="frmesrc" type="host">*.payu.in</value>
            </values>
        </policy>
		<policy id="frame-ancestors">
            <values>
                <value id="self" type="host">'self'</value>
		<value id="payu" type="host">*.payu.in</value>
            </values>
        </policy>
		<policy id="form-action">
            <values>
                <value id="fra" type="host">'self'</value>
		<value id="frmact" type="host">*.payu.in</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
